{
  "name": "n8n-nodes-tiendanube-integration",
  "version": "1.6.4",
  "description": "n8n nodes for Tiendanube (Nuvemshop) API integration",
  "keywords": [
    "n8n-community-node-package"
  ],
  "license": "MIT",
  "homepage": "",
  "author": {
    "name": "Community",
    "email": "community@n8n.io"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "format": "prettier nodes/**/*.ts credentials/**/*.ts --write",
